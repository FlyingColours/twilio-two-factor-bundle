{
    "name": "flyingcolours/twilio-two-factor-bundle",
    "description": "Use Twilio for 2FA (SMS or voice) with Symfony and scheb/two-factor-bundle",
    "type": "library",
    "require": {
        "php": "^7.2",
        "scheb/two-factor-bundle": "*",
        "symfony/framework-bundle": "~2.3||~3",
        "twilio/sdk": "^5"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.3",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.0",
        "symfony/templating": "~2.3||~3",
        "leanphp/phpspec-code-coverage": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "FlyingColours\\TwilioTwoFactorBundle\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.2"
        }
    }
}
